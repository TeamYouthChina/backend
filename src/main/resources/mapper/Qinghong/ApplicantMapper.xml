    <!--添加一个职位收藏-->
    <insert id="addJobCollect" parameterType="com.youthchina.domain.Qinghong.JobCollect" useGeneratedKeys="true" keyProperty="collect_id">
        INSERT INTO STU_JOB_COLLECT
        (JOB_ID,JOB_COLL_TIME,STU_ID,IS_DELETE,IS_DELETE_TIME)
