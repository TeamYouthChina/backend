    <!--获取用户所有基本信息-->
    <select id="getUserInfo" resultType="com.youthchina.domain.Qinghong.UserInfo">
        select *
        from SYS_USER
        where USER_ID=#{user_id}
    </select>

    <!--通过user_id获取stu_id-->
    <select id="getBaseInfo" resultType="com.youthchina.domain.Qinghong.BaseInfo">
        SELECT *
        FROM STU_INFO
        WHERE USER_ID=#{user_id} AND IS_DELETE=0
    </select>

    <!--获取申请者的信息-->
    <select id="getStudentInfo" resultMap="studentResultMap">
        select a.*,b.*,c.*,d.*,e.*,f.*,g.*,z.*,o.*,p.*,m.*
        from STU_INFO a
        LEFT JOIN SYS_USER z ON a.USER_ID=z.USER_ID AND z.IS_DELETE=0
        LEFT JOIN STU_EDU_INFO b ON a.STU_ID=b.STU_ID AND b.IS_DELETE=0
        LEFT JOIN SYS_DEGREE p ON b.EDU_DEGREE=p.DEGREE_NUM
        LEFT JOIN STU_SUB_INFO c ON a.STU_ID=c.STU_ID AND c.IS_DELETE=0
        LEFT JOIN STU_PROJECT d ON a.STU_ID=d.STU_ID AND d.IS_DELETE=0
        LEFT JOIN STU_WORK e ON a.STU_ID=e.STU_ID AND e.IS_DELETE=0
        LEFT JOIN STU_ACTIVITY f ON a.STU_ID=f.STU_ID AND f.IS_DELETE=0
        LEFT JOIN STU_CERTIFICATE g ON a.STU_ID=g.STU_ID AND g.IS_DELETE=0
        LEFT JOIN STU_ADVAN_LABEL n ON a.STU_ID=n.STU_ID AND n.IS_DELETE=0
        LEFT JOIN SYS_ADVAN_LABEL_CLASS o ON o.LABEL_CODE=n.LABEL_CODE AND o.IS_DELETE=0
        WHERE a.USER_ID=#{id} AND a.IS_DELETE=0
    </select>

    <!--获取申请者的项目信息-->
    <select id="getProjects" resultType="com.youthchina.domain.Qinghong.Project">
        SELECT b.*
        FROM STU_INFO a,STU_PROJECT b
        WHERE a.USER_ID=#{user_id} AND a.STU_ID=b.STU_ID AND a.IS_DELETE=0 AND b.IS_DELETE=0
    </select>

    <!--获取申请者的教育信息-->
    <select id="getEducations" resultType="com.youthchina.domain.Qinghong.EducationInfo">
        SELECT b.*
        FROM STU_INFO a,STU_EDU_INFO b
        WHERE a.USER_ID=#{user_id} AND a.STU_ID=b.STU_ID AND a.IS_DELETE=0 AND b.IS_DELETE=0
    </select>

    <!--获取申请者的工作经历信息-->
    <select id="getWorks" resultType="com.youthchina.domain.Qinghong.Work">
        SELECT b.*
        FROM STU_INFO a,STU_WORK b
        WHERE a.USER_ID=#{user_id} AND a.STU_ID=b.STU_ID AND a.IS_DELETE=0 AND b.IS_DELETE=0
    </select>

    <!--获取申请者的认证信息-->
    <select id="getCertificates" resultType="com.youthchina.domain.Qinghong.Certificate">
        SELECT b.*
        FROM STU_INFO a
                 LEFT JOIN STU_CERTIFICATE b ON a.STU_ID=b.STU_ID
        WHERE a.USER_ID=#{user_id} AND a.IS_DELETE=0 AND b.IS_DELETE=0
    </select>

    <!--获取申请者的活动信息-->
